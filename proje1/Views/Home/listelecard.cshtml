@model List<proje1.Models.Domain.Person>

<div class=" container text-center">
    <div class="row">
        @foreach (var ogrenciler in Model)
        {
            <div class="col-md-4 mt-5">
                <div class="card" style="width:  18rem;">
                    <img class="card-img-top" src="~/resim/@ogrenciler.resim" />
                    <div class="dsa">
                        <h5 class="card-title">@ogrenciler.Ad</h5>
                        <p class="card-text">@ogrenciler.Id</p>
                        <a class="btn btn-success" href="/Home/Privacy">anasayfa2</a>
                    </div>
                </div>


            </div>

        }
    </div>
</div>

